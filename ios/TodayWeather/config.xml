<?xml version='1.0' encoding='utf-8'?>
<widget id="net.wizardfactory.todayweather" version="0.9.10" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="HandleOpenUrl">
        <param name="ios-package" value="CDVHandleOpenURL" />
        <param name="onload" value="true" />
    </feature>
    <feature name="IntentAndNavigationFilter">
        <param name="ios-package" value="CDVIntentAndNavigationFilter" />
        <param name="onload" value="true" />
    </feature>
    <feature name="GestureHandler">
        <param name="ios-package" value="CDVGestureHandler" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Device">
        <param name="ios-package" value="CDVDevice" />
    </feature>
    <feature name="Console">
        <param name="ios-package" value="CDVLogger" />
    </feature>
    <feature name="SplashScreen">
        <param name="ios-package" value="CDVSplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Keyboard">
        <param name="ios-package" onload="true" value="IonicKeyboard" />
    </feature>
    <feature name="Geolocation">
        <param name="ios-package" value="CDVLocation" />
    </feature>
    <feature name="StatusBar">
        <param name="ios-package" value="CDVStatusBar" />
        <param name="onload" value="true" />
    </feature>
    <feature name="InAppBrowser">
        <param name="ios-package" value="CDVInAppBrowser" />
    </feature>
    <feature name="SocialSharing">
        <param name="ios-package" value="SocialSharing" />
        <param name="onload" value="true" />
    </feature>
    <feature name="PaymentsPlugin">
        <param name="ios-package" value="PaymentsPlugin" />
    </feature>
    <feature name="PushNotification">
        <param name="ios-package" value="PushPlugin" />
    </feature>
    <feature name="AdMob">
        <param name="ios-package" value="CDVAdMobPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="NetworkStatus">
        <param name="ios-package" value="CDVConnection" />
    </feature>
    <feature name="AppPreferences">
        <param name="ios-package" value="AppPreferences" />
    </feature>
    <feature name="AppVersion">
        <param name="ios-package" value="AppVersion" />
    </feature>
    <feature name="UniversalAnalytics">
        <param name="ios-package" value="UniversalAnalyticsPlugin" />
    </feature>
    <name>TodayWeather</name>
    <description>
        Inform how warm or cold it is today than yesterday.
    </description>
    <author email="todayweather@wizardfactory.net" href="https://github.com/WizardFactory">
        WizardFactory TodayWeather Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <icon src="resources/android/icon/drawable-xhdpi-icon.png" />
    <allow-navigation href="*" />
    <allow-intent href="mailto:*" />
    <config-file parent="NSAppTransportSecurity" platform="ios" target="*-Info.plist">
        <dict>
            <key>NSAllowsArbitraryLoads</key>
            <true />
        </dict>
    </config-file>
    <config-file parent="CFBundleDevelopmentRegion" platform="ios" target="*-Info.plist">
        <string>ko_KR</string>
    </config-file>
    <config-file parent="CFBundleLocalizations" platform="ios" target="*-Info.plist">
        <array>
            <string>en</string>
            <string>ko</string>
        </array>
    </config-file>
    <splash height="1136" src="resources/ios/splash/Default-568h@2x~iphone.png" width="640" />
